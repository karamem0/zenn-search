{
  "automock": false,
  "setupFiles": [
    "./jest.setup.ts"
  ],
  "snapshotSerializers": [
    "@emotion/jest/serializer"
  ],
  "testEnvironment": "jsdom",
  "testMatch": [
    "**/*.test.ts",
    "**/*.test.tsx"
  ],
  "transform": {
    "^.+\\.[tj]sx?$": "babel-jest"
  },
  "transformIgnorePatterns": [
    "/node_modules/(?!@fluentui)"
  ]
}
